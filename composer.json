{
    "name": "gemorroj/linfo",
    "description": "App and library for easily parsing system information, like network/torrents/cpu/memory/usb/pci/sound cards/filesystems/raid array/ipmi/etc.",
    "keywords": [
        "health",
        "system",
        "linux",
        "specs",
        "cpu"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Joe Gillotti",
            "email": "joe@joegillotti.com",
            "homepage": "http://joegillotti.com",
            "role": "Author"
        },
        {
            "name": "Gemorroj",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-pcre": "*",
        "symfony/process": "^3.3|^4.0"
    },
    "suggest": {
        "ext-mbstring": "On Windows"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27|^6.5.7|^7.0.2",
        "phpstan/phpstan": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "Linfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linfo\\Tests\\": "tests/"
        }
    }
}

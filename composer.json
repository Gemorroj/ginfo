{
    "name": "gemorroj/ginfo",
    "description": "App and library for easily parsing system information, like network/cpu/memory/usb/pci/sound cards/filesystems/raid array/ipmi/etc.",
    "keywords": [
        "health",
        "system",
        "linux",
        "specs",
        "cpu",
        "memory",
        "disk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Gillotti",
            "email": "joe@joegillotti.com",
            "homepage": "http://joegillotti.com",
            "role": "Author"
        },
        {
            "name": "Gemorroj",
            "role": "Author"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-pcre": "*",
        "ext-mbstring": "*",
        "symfony/process": "^4.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^7.1.1",
        "phpstan/phpstan": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "Ginfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ginfo\\Tests\\": "tests/"
        }
    }
}

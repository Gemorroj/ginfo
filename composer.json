{
    "name": "gemorroj/ginfo",
    "description": "Library for parsing system information, like network/cpu/memory/usb/pci/sound cards/filesystems/raid array/ipmi/etc and php/web-servers/databases.",
    "keywords": [
        "health",
        "info",
        "system",
        "php",
        "server",
        "linux",
        "specs",
        "cpu",
        "memory",
        "disk",
        "monitoring",
        "redis",
        "memcache",
        "memcached",
        "mongo",
        "mongodb",
        "valkey",
        "mysql",
        "mariadb",
        "postgres",
        "manticore",
        "elasticsearch",
        "opensearch",
        "sqlite",
        "nginx",
        "angie",
        "caddy",
        "apache",
        "httpd",
        "opcache",
        "apcu",
        "fpm"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Gemorroj",
            "role": "Author"
        },
        {
            "name": "Joe Gillotti",
            "email": "joe@joegillotti.com",
            "homepage": "http://joegillotti.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-pcre": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "symfony/process": "^6.4.20||^7.0.8",
        "symfony/http-client": "^6.4.20||^7.0.8"
    },
    "require-dev": {
        "ext-pdo_mysql": "*",
        "ext-pdo_pgsql": "*",
        "ext-pdo_sqlite": "*",
        "ext-redis": "*",
        "ext-apcu": "*",
        "ext-memcached": "*",
        "ext-mongodb": "*",
        "phpunit/phpunit": "^11.5.19",
        "phpstan/phpstan": "^2.1.14",
        "friendsofphp/php-cs-fixer": "^3.75.0"
    },
    "suggest": {
        "ext-pdo_mysql": "For MySQL/MariaDB/Manticore",
        "ext-pdo_pgsql": "For Postgres",
        "ext-pdo_sqlite": "For Sqlite",
        "ext-redis": "For Redis/Valkey",
        "ext-memcached": "For Memcached",
        "ext-apcu": "For APCU",
        "ext-mongodb": "For MongoDB"
    },
    "autoload": {
        "psr-4": {
            "Ginfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ginfo\\Tests\\": "tests/"
        }
    }
}

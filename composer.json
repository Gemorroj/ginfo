{
    "name": "gemorroj/ginfo",
    "description": "Library for parsing system information, like network/cpu/memory/usb/pci/sound cards/filesystems/raid array/ipmi/etc.",
    "keywords": [
        "health",
        "system",
        "linux",
        "specs",
        "cpu",
        "memory",
        "disk",
        "monitoring"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Gemorroj",
            "role": "Author"
        },
        {
            "name": "Joe Gillotti",
            "email": "joe@joegillotti.com",
            "homepage": "http://joegillotti.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-pcre": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "symfony/process": "^6.4||^7.0"
    },
    "require-dev": {
        "ext-pdo": "*",
        "phpunit/phpunit": "^11.5.17",
        "phpstan/phpstan": "^2.1.11",
        "friendsofphp/php-cs-fixer": "^3.75.0"
    },
    "autoload": {
        "psr-4": {
            "Ginfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ginfo\\Tests\\": "tests/"
        }
    }
}
